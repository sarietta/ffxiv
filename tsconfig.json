    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "module": "es2015",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "target": "es5",
    "lib": ["es2015", "es2017", "dom"]
  },
  "include": [
    "./src/**/*"
  ]
}
